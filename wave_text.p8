pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--wave text by tashettrash

--set variables
--text you want to write
text = "wave text wave text wave text"
speed = 25
height = 15
tim = 0
--draw text
function _draw()
--clear screen
 cls()
--update time
 tim += 1
--draw text
 for i=0,#text,1 do
  print(sub(text,i,i),
  1+(i*4),
  64+sin((tim+i)/speed)*height,7)
 end
end
